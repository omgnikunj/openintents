<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.openintents.samples.presentpicker">
    
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <application android:icon="@drawable/present_application001a">
        <activity android:name=".PresentPicker" android:label="@string/presentpicker_app_name">
             <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
				<category android:name="org.openintents.category.MAIN" />
				
            </intent-filter>
        </activity>
    </application>
</manifest> 