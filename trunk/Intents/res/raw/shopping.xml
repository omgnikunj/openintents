<?xml version="1.0" encoding="utf-8"?>
<package name="org.openintents.shopping">
	<dir
		name="Shopping List"
		uri="content://org.openintents.shopping/lists/"
		text-columns="name"
		time-column="created">
		<intent
			uri="{url}"
			action="android.intent.action.VIEW"/>
	</dir>
	<dir
		name="Shopping Items"
		uri="content://org.openintents.shopping/items/"
		text-columns="name"
		time-column="created">
		<intent
			uri="{url}"
			action="android.intent.action.VIEW"/>
	</dir>
</package>
