<?xml version="1.0" encoding="utf-8"?>
<package name="com.google.android.notepad">
	<dir
		name="NotePad"
		uri="content://com.google.provider.NotePad/notes"
		text-columns="title,note"
		time-column="modified">
		<intent
			uri="content://com.google.provider.NotePad/notes/{_id}"
			action="android.intent.action.EDIT"/>
	</dir>
</package>