<?xml version="1.0" encoding="utf-8"?>
<package name="com.google.android.contacts">
	<dir
		name="Contacts"
		uri="content://contacts/people/"
		text-columns="name,notes,number"
		id-column="people._id">
		<intent
			uri="content://contacts/people/{_id}"
			action="android.intent.action.VIEW"/>
	</dir>
	<dir
		name="Contact methods"
		uri="content://contacts/contact_methods/"
		text-columns="people.name,data"
		id-column="contact_methods._id">
		<intent
			uri="content://contacts/contact_methods/{_id}"
			action="android.intent.action.VIEW"/>
	</dir>
</package>